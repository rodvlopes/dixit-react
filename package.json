  "scripts": {
    "start": "webpack-dev-server -d --mode development --open --hot",
    "ws": "node server/ws.js",
    "build": "rm public/main*.js; webpack -p",
    "deploy": "rsync -azh --delete --progress -e 'ssh -p 2222' ./public/* deploy@v.rodrigolop.es:/var/www/moran.ga/dixit; scp -P 2222 server/ws.js deploy@v.rodrigolop.es:/home/deploy/dixitWs.js",
